{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@access/*": ["src/access/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@providers/*": ["src/providers/*"],
      "@utils/*": ["src/utils/*"],
      "@theme/*": ["src/theme/*"],
      "@shared/*": ["../shared/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
  // "exclude": ["**/__test__/*"]
}
